<h1>Conquistas Cadastrados</h1>

<a href="/jogos/{{jogo.id}}/novaConquista">Cadastrar Conquista</a>

<table border="1">
  <!-- table row -->
  <tr>
    <!-- table head -->
    <th>Titulo</th>
    <th>Conquista</th>
  </tr>
  {{#each conquistas}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.conquista}}</td>
    </tr>
  {{/each}}
</table>